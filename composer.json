{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/doctrine-bundle": "^2.8",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.14",
    "easycorp/easyadmin-bundle": "^4.5",
    "nelmio/security-bundle": "^3.0",
    "surfnet/stepup-saml-bundle": "dev-feature/support-sf6-dependencies",
    "symfony/console": "6.2.*",
    "symfony/dotenv": "6.2.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "6.2.*",
    "symfony/http-foundation": "6.2.*",
    "symfony/monolog-bundle": "^3.0",
    "symfony/runtime": "6.2.*",
    "symfony/webpack-encore-bundle": "^1.16",
    "symfony/yaml": "6.2.*"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^7.1",
    "doctrine/coding-standard": "^11.0",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "overtrue/phplint": "^5.1",
    "phpmd/phpmd": "^2.12",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-doctrine": "^1.3",
    "phpstan/phpstan-phpunit": "^1.1",
    "phpstan/phpstan-strict-rules": "^1.2",
    "phpstan/phpstan-symfony": "^1.1",
    "phpunit/phpunit": "^9.6",
    "qossmic/deptrac-shim": "^1.0.0",
    "sebastian/phpcpd": "^6.0",
    "symfony/browser-kit": "6.2.*",
    "symfony/css-selector": "6.2.*",
    "symfony/debug-bundle": "6.2.*",
    "symfony/maker-bundle": "^1.43",
    "symfony/phpunit-bridge": "^6.0",
    "symfony/stopwatch": "6.2.*",
    "symfony/web-profiler-bundle": "6.2.*"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "check": [
      "@phplint",
      "@phpcs",
      "@phpcpd",
      "@phpstan",
      "@dsv",
      "@test",
      "@composer audit"
    ],
    "dsv": "./bin/console doctrine:schema:validate --skip-sync",
    "phplint": "./tools/qa/phplint",
    "phpcs": "./tools/qa/phpcs",
    "phpcbf": "./tools/qa/phpcbf",
    "phpmd": "./tools/qa/phpmd",
    "phpstan": "./tools/qa/phpstan",
    "deptrac": "./tools/qa/deptrac",
    "phpcpd": "./tools/qa/phpcpd",
    "test": "./tools/qa/phpunit"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "6.2.*",
      "docker": true
    }
  }
}
